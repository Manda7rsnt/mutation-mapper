{
  "name": "mutation-mapper",
  "version": "2.0.0",
  "author": "",
  "license": "",
  "scripts": {
    "start": "NODE_ENV=development webpack --progress --colors --watch",
    "dev": "NODE_ENV=development webpack --progress --colors",
    "dev-global-ext": "EXT_LIB=global NODE_ENV=development webpack --progress --colors",
    "dev-module-ext": "EXT_LIB=module NODE_ENV=development webpack --progress --colors",
    "prod": "NODE_ENV=production webpack --progress --colors",
    "prod-global-ext": "EXT_LIB=global NODE_ENV=production webpack --progress --colors",
    "prod-module-ext": "EXT_LIB=module NODE_ENV=production webpack --progress --colors",
    "build-debug": "NODE_ENV=development webpack --config debug.webpack.config.js --progress --colors",
    "build-dev": "npm run dev && npm run build-debug",
    "build-ext-dev": "npm run module-ext-dev && npm run build-debug",
    "serve": "webpack-dev-server --inline --hot",
    "clean": "rimraf build",
    "build": "npm run clean && npm run prod",
    "eslint": "eslint src/** --ext .js --ext .jsx --cache"
  },
  "dependencies": {
    "jquery": "^2.0.3",
    "jquery-ui": "^1.12.1",
    "jquery-flesler-scrollto": "^2.0.0",
    "jquery-expander": "^1.7.0",
    "underscore": "^1.8.3",
    "backbone": "^1.1.0",
    "d3": "^3.0.0",
    "datatables.net": "^1.10.0",
    "datatables.net-dt": "^1.10.0",
    "datatables-tabletools": "^2.2.2",
    "drmonty-datatables-colvis": "^1.1.2",
    "qtip2": "^3.0.0",
    "filesaver.js-npm": "^1.0.1"
  },
  "devDependencies": {
    "webpack": "^1.13.1",
    "circular-dependency-plugin": "^2.0.0",
    "webpack-dev-server": "^1.15.0",
    "imports-loader": "^0.6.5",
    "exports-loader": "^0.6.3",
    "style-loader": "^0.12.3",
    "file-loader": "^0.8.4",
    "css-loader": "^0.16.0",
    "eslint": "^3.9.0",
    "gulp": "^3.9.0",
    "del": "^2.2.0",
    "vinyl-paths": "^2.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-htmlmin": "^1.1.4",
    "gulp-replace": "^0.5.4",
    "gulp-template-compile": "^1.0.0",
    "gulp-uglify": "^1.4.1"
  }
}
